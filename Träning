import csv

kameraData = []
platsData = []


def read_filer(file_name): #öppna csv-fil som anges

    with open("file_name","r") as r_file:
        csv_reader = csv.reader(r_file, delimiter =';') #läser filen, delimiter tar ; för det kommer emellan annars. 
    
    for rad in csv_reader: #Lägger in data från CSV till den tomma listan 
        file_name.appen(rad)
    return file_name
